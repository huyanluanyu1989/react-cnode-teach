{
  "parser": "babel-eslint",
  "env": {
    "node": true,
    "es6": true,
    "browser": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "extends": "airbnb",
  "rules": {
    "semi": "off",
    "react/jsx-filename-extension": "off",
    "react/require-default-props": "off",
    "react/prefer-stateless-function": "off",
    "react/no-string-refs": "off",
    "comma-dangle": [2, "never"],
    "jsx-quotes": "off"
  }
}
